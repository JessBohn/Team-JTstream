<h1>Log in</h1>

<%= form_tag sessions_path do %>
  <p>
    <%= label_tag :email %>
    <%= text_field_tag :email, params[:email], :required => true %>
  </p>
  <p>
    <%= label_tag :password %>
    <input type="password" name="password" required/>
  </p>
  <%= submit_tag "Log in", class: 'button-primary' %>
<% end %>
